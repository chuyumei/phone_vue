            console.log(email)\r\n                if(email==\"\"){\r\n                    Toast(\"请设置您的邮箱\")\r\n                    return;\r\n                }\r\n                var phone=this.phone\r\n                if(phone==\"\"){\r\n                    Toast(\"请设置您的手机号\")\r\n                    return;\r\n                }\r\n                var upwd=this.upwd\r\n                if(upwd==\"\"){\r\n                    Toast(\"请设置您的密码\")\r\n                    return;\r\n                }\r\n                this.$axios.post(\"register?email=\"+email+\"&phone=\"+phone+\"&upwd=\"+upwd+\"&uname=\"+uname).then(result=>{\r\n                    console.log(result)\r\n                })\r\n            },\r\n        },\r\n        created(){\r\n\r\n        },\r\n    }\r\n</script>\r\n<style>\r\n    .register .mint-field .mint-cell-value{\r\n        margin-left: -10px;\r\n        margin-right: 18px;\r\n    }\r\n    .register .mint-button--primary{\r\n        background-color: lightsteelblue;\r\n        width: 72%;\r\n        margin-left: -1px;\r\n        margin-top: 10px;\r\n    }\r\n    .register .mint-cell-wrapper{\r\n        height:36px;\r\n        width: 76%;\r\n        font-size:13px;\r\n        margin-left: 50px;\r\n    }\r\n    .register .mint-cell-value input{\r\n        width: 234px;\r\n        border:0;\r\n        background-color: ghostwhite;\r\n        margin-bottom: 0;\r\n        border: 1px solid lightblue;\r\n    }\r\n    .register .mint-field .mint-cell-title{\r\n        height: 64px;\r\n        line-height: 4;\r\n    }       \r\n</style>\r\n\r\n"],"sourceRoot":"src\\components\\user"}]}