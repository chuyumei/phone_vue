er -->\r\n    <mt-tab-container v-model=\"selected\">\r\n    <!--*************************************介绍*******************************************************************-->\r\n      <mt-tab-container-item id=\"1\">\r\n        <div class=\"cake-details\">\r\n        <!--1:商品购买区域-->\r\n          <div class=\"mui-card\">\r\n            \r\n         \r\n            <div class=\"mui-card-content\">\r\n              <div class=\"mui-card-content-inner\">\r\n                <p class=\"top-title\">{{details[0].title}}</p>\r\n                <p class=\"price\">\r\n                    <span class=\"now\">￥{{details[0].price}}</span>\r\n                </p>\r\n                <!--2:商品参数区域-->\r\n                <div class=\"par\">\r\n                    <p class=\"t\">【课程介绍】:</p>\r\n                    <p>{{details[0].cover}}</p>\r\n                    <p class=\"t\">【适合人群】:</p>\r\n                    <p>{{details[0].person}}</p>\r\n                    <p class=\"t\">【学习指南】:</p>\r\n                    <p>{{details[0].help}}</p>\r\n                    <p><span class=\"t\">【讲师】:</span>\r\n                        <span> <img :src=\"details[0].img3\" class=\"img3\"/></span>\r\n                        <span>{{details[0].teacher}}</span>\r\n                    </p>\r\n                </div>\r\n              </div>\r\n            </div>\r\n\t\t      </div>\r\n        <!--3:中间大图片-->\r\n        <ul class=\"details\">\r\n          <li>\r\n            <img :src=\"details[0].img2\">\r\n          </li>\r\n        </ul>\r\n       <!--4:购买数量，加入购物车-->\r\n        <div>\r\n          <p>购买数量:\r\n            <div class=\"mui-numbox\" data-numbox-min='1' data-numbox-max='9'>\r\n                <button class=\"mui-btn mui-btn-numbox-minus\" type=\"button\" @click=\"cakeSub\">-</button>\r\n                <input id=\"test\" class=\"mui-input-numbox\" type=\"number\" value=\"1\" v-model=\"val\" />\r\n                <button class=\"mui-btn mui-btn-numbox-plus\" type=\"button\" @click=\"cakeAdd\">+</button>\r\n            </div>\r\n          </p>\r\n          <p>\r\n            <mt-button type=\"primary\" size=\"small\">立即购买</mt-button>\r\n            <mt-button type=\"danger\" size=\"small\" @click=\"addcart()\">加入购物车</mt-button>\r\n          </p>\r\n        </div>      \r\n        <!--*************************************目录*********************************************************************-->\r\n    </div>\r\n      </mt-tab-container-item>\r\n      <mt-tab-container-item id=\"2\">\r\n        \r\n      </mt-tab-container-item>\r\n      <!--*************************************用户评论***********************************************************-->\r\n      <mt-tab-container-item id=\"3\">\r\n        <div class=\"comment-container\">\r\n          <!--用户评论-->\r\n          <textarea placeholder=\"请吐槽最多120字\" maxlength=\"120\" v-model=\"msg\"></textarea> \r\n          <mt-button size=\"large\" @click=\"pushComment\">发表评论</mt-button>\r\n          <div class=\"cmt-list\" v-for=\"(item,i) in list\">\r\n              <div class=\"cmt-item\">\r\n                  <div class=\"cmt-info\">第一楼{{i+1}}{{item.user_name}}发表时间：{{item.ctime | timeFilter}}</div>\r\n                  <div class=\"cmt-body\">{{item.content}}</div>\r\n              </div>\r\n          </div>\r\n          <mt-button size=\"large\" @click=\"getMore\">加载更多</mt-button>\r\n        </div>   \r\n     <!--评论列表-->\r\n        <div class=\"app-newinfo\">\r\n          <h3>{{info.title}}</h3>\r\n          <div>{{info.content}}</div>\r\n       \r\n        </div>\r\n\r\n      </mt-tab-container-item>\r\n    </mt-tab-container>\r\n  </div>\r\n</template>\r\n \r\n <style>\r\n    .nav .mint-tab-item-label{\r\n      font-size:17px !important;\r\n    }\r\n </style>\r\n<script>\r\n//import details from \"../cakes/Cakedetails.vue\"\r\n//import twolist from \"./twolist.vue\"\r\n//import comment from \"../pub/comment.vue\"\r\nimport {Toast} from \"mint-ui\"\r\n\r\nexport default {\r\n  name: 'page-navbar',\r\n \r\n  data() {\r\n    return {\r\n      selected: '1',\r\n      details:[],\r\n      id:this.$route.query.id,\r\n      list:[],\r\n      msg:\"\" ,\r\n      val:1,\r\n      info:{},\r\n    };\r\n  },\r\n  methods:{\r\n    getNewsInfo(){\r\n      //发送ajax请求获取当前新闻详细信息\r\n      var url = \"http://127.0.0.1:3000/newsinfo\";\r\n      this.$http.get(url).then(result=>{\r\n          console.log(result.body);\r\n          this.info = result.body;\r\n      })\r\n    },\r\n    addcart(){\r\n      var id=this.$route.query.id              \r\n      var count=this.val\r\n      this.$http.get(\"addCartTo?pid=\"+id+\"&count=\"+count).then(result=>{\r\n      //console.log(result)\r\n        if(result.body.code==1){\r\n            this.$store.commit(\"increment\",count);\r\n            Toast(result.body.msg)\r\n        }else{\r\n            Toast(result.body.msg)\r\n        }      \r\n      })\r\n    },\r\n    getshopCart(){\r\n      this.$http.get(\"shopCart\").then(result=>{\r\n          //console.log(result)\r\n      })\r\n    },\r\n    cakeSub(){\r\n      if(this.val>0)\r\n      this.val--;\r\n    },\r\n    cakeAdd(){\r\n      this.val++\r\n    },\r\n    pushComment(){\r\n      var cid=this.$route.query.id;\r\n      var cmsg=this.msg\r\n      if(cmsg.trim().length==0){\r\n          Toast(\"评论内容不能为空\")\r\n          return;\r\n      }\r\n      this.$http.post(\"addComment\",{cid:cid,cmsg:cmsg}).then(result=>{\r\n          console.log(result)\r\n          Toast(\"评论成功\")\r\n          \r\n      })\r\n    },\r\n    getImgs(){\r\n      var id=this.$route.query.id;\r\n          //console.log(id)\r\n      this.$http.get(\"cakelists2?id=\"+id).then(result=>{\r\n          this.details=result.body; \r\n          console.log(this.details)\r\n      })\r\n    },\r\n  },  \r\n    created(){\r\n        this.getImgs(),\r\n        this.getNewsInfo();\r\n    }, \r\n\r\n  components:{\r\n            //\"details-box\":details,\r\n            //\"twolist-box\":twolist,\r\n            //\"comment-box\":comment\r\n        }\r\n};\r\n</script>\r\n\r\n"],"sourceRoot":"src\\components\\pub"}]}