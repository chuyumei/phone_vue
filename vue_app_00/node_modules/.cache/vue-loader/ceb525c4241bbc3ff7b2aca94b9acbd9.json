\">\r\n                    <h3 class=\"title\">{{item.title}}</h3>\r\n                    <h3 class=\"text\">{{item.text}}</h3>\r\n                    <p class=\"price\">\r\n                        <span>￥{{item.price}}</span>\r\n                        <span class=\"size\">/{{item.size}}</span>\r\n                    </p>\r\n                </div> \r\n            </li>\r\n        </ul>\r\n    </div>\r\n</template>\r\n\r\n\r\n<style scoped>\r\n    .twolist{\r\n        background:#fff;\r\n    }\r\n    .twolist ul li{\r\n        float:left;\r\n        margin:0;\r\n        padding:0;\r\n    }\r\n    .twolist .goods-item{\r\n        margin-top:13px;\r\n        width: 200px;\r\n        height: 212px;\r\n    }\r\n    .twolist .goods-item .info{\r\n        width: 171px;\r\n        margin-left:8px;\r\n        text-align:center;\r\n    }\r\n    .twolist .goods-item .info .cart{\r\n        width:10%;\r\n        float: right;\r\n    }\r\n    .twolist .goods-item img{\r\n        width:96%;\r\n    }\r\n    .twolist .goods-item .title{\r\n        font-size: 13px;\r\n        margin-bottom:0;\r\n        color: lightcoral;\r\n    }\r\n    .twolist .goods-item .text{\r\n        font-size: 1px !important;\r\n        font-weight: normal;\r\n        color: #aaa;\r\n        display: inline-block;\r\n        margin:0;\r\n        overflow: hidden;\r\n        height: 12px;\r\n    }\r\n    .twolist .goods-item .zz{\r\n        margin:0;\r\n        font-size: 1px !important;\r\n        color:rgb(214,95,136);\r\n    }\r\n    .twolist .goods-item .zz .zy{\r\n        border:1px solid rgb(241,170,222);\r\n        border-radius:4px;\r\n    }\r\n    .twolist .goods-item .zz .jj{\r\n        border:1px solid rgb(241,170,222);\r\n        margin-left: 31px;\r\n        border-radius:4px;\r\n    }\r\n    .twolist .goods-item .price{\r\n        color:rgb(252,63,0);\r\n        margin:0;\r\n    }\r\n    .twolist .goods-item .price .size{\r\n        color:#aaa;\r\n        padding-left:3px;\r\n    }\r\n</style>\r\n\r\n<script>\r\n    export default {\r\n        data(){\r\n            return {\r\n                list:[]\r\n            }\r\n        },\r\n        methods:{\r\n            loadMore() {\r\n                this.loading = true;\r\n                setTimeout(() => {\r\n                    let last = this.list[this.list.length - 1];\r\n                    for (let i = 1; i <= 6; i++) {\r\n                    this.list.push(last + i);\r\n                    }\r\n                    this.loading = false;\r\n                }, 2500);\r\n            },\r\n\r\n\r\n            getListCake(){\r\n                this.$http.get(\"cakelists\").then(result=>{\r\n                this.list=result.body\r\n                console.log(this.list)\r\n                })\r\n           },   \r\n        },\r\n        created(){\r\n            this.getListCake();\r\n            this.loadMore();\r\n        }\r\n    }\r\n</script>"]}]}