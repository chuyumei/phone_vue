Container.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAkIA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"cartContainer.vue","sourceRoot":"src\\components\\cakes","sourcesContent":["<template>\r\n    <div class=\"list-container\">\r\n        <ul class=\"mui-table-view\">\r\n\t\t\t<li class=\"mui-table-view-cell mui-media\" v-for=\"item in cartlist\" :key=\"item.id\">\r\n\t\t\t\t<a href=\"javascript:;\">\r\n\t\t\t\t\t<div class=\"mui-media-body\">\r\n\t\t\t\t\t\t<p class='mui-ellipsis'>\r\n                        <input type=\"checkbox\" @click=\"itemSelected(item.id)\" v-model=\"item.checked\"  name=\"all\"/></button>\r\n                        <img src=\"http://127.0.0.1:3000/img/lists/cd8fb7b2c1cc481893f5bcdea46148dc.jpeg\">\r\n                        \t{{item.title}}\r\n                            <span class=\"price\">{{item.price}}</span>\r\n                            <span class=\"count\">\r\n                                <!--2.2mui 数字按钮-->\r\n                                <div class=\"mui-numbox\" data-numbox-min='1' data-numbox-max='9'>\r\n                                    <button class=\"mui-btn mui-btn-numbox-minus left\" type=\"button\" @click=\"deccart(item.id)\">-</button>\r\n                                    <input id=\"test\" class=\"mui-input-numbox\" type=\"number\" :value=\"item.count\"/>\r\n                                    <button class=\"mui-btn mui-btn-numbox-plus right\" type=\"button\" @click=\"inccart(item.id)\">+</button>\r\n                                </div>\r\n                            </span>\r\n\t\t\t\t\t\t</p>\r\n\t\t\t\t\t</div>\r\n\t\t\t\t</a>\r\n\t\t\t</li>\r\n       \t</ul>\r\n        <div>\r\n            <input type=\"checkbox\" @click=\"selectedAll()\" v-model=\"allChecked\"/>\r\n        </div>\r\n        <div class=\"sumscore\">总价 ：￥\r\n            <span v-if=\"displaymoney=true\">{{priceSum}}</span>\r\n            <span v-else=\"displaymoney=false\">0</span>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport twolist from \"../pub/twolist.vue\"\r\n\r\n\r\n    export default {\r\n        data(){\r\n            return {\r\n                cartlist:[\r\n                    {id:1,title:\"华为p10\",price:3999,count:2},\r\n                    {id:2,title:\"华为p11\",price:4999,count:1},\r\n                    {id:3,title:\"华为p12\",price:5999,count:1}\r\n                ],\r\n                allChecked:true,\r\n                checked:false,\r\n            }\r\n        },\r\n        methods:{    \r\n            //单选按钮\r\n            itemSelected:function(item){\r\n                console.log(\"我是单选按钮\")\r\n                console.log(item)\r\n                \r\n                if(item.checked==true){\r\n                    \r\n                    for (let i = 0; i < this.cartlist.length; i++) {\r\n                        if(this.cartlist[i].checked==!true){ \r\n                        return;\r\n                        }\r\n                    }\r\n                    this.allChecked=true \r\n                }else{\r\n                this.allChecked=false\r\n                }\r\n            },   \r\n\r\n            //全选按钮\r\n            selectedAll:function(item){\r\n                if(this.allChecked==false){\r\n                    for(var i=0;i<this.cartlist.length;i++){\r\n                        var item = this.cartlist[i];\r\n\t\t\t\t\t\titem.checked = true;\r\n                    }\r\n                }else{\r\n                    for(var i=0;i<this.cartlist.length;i++){\r\n                        var item = this.cartlist[i];\r\n\t\t\t\t\t\titem.checked = false;\r\n                    }\r\n                }\r\n                this.allChecked = !this.allChecked;\r\n            },\r\n            deccart(id){\r\n                for(var item of this.cartlist){\r\n                    //console.log(item.id)\r\n                    if(item.id==id){\r\n                        if(item.count<2)\r\n                        return; \r\n                        item.count--;\r\n                        break;\r\n                    }\r\n                }    \r\n            },\r\n            inccart(id){\r\n                for(var item of this.cartlist){\r\n                    if(item.id==id){\r\n                        item.count++;\r\n                        break;\r\n                    }\r\n                }  \r\n            },\r\n            shopCartList(){\r\n                this.$http.get(\"shopCart\").then(result=>{\r\n                    this.cartlist=result.body;\r\n                    console.log(this.cartlist)\r\n                })\r\n            },\r\n        },\r\n        created(){\r\n            //this.shopCartList()\r\n        },\r\n        computed:{\r\n            priceSum:function(){\r\n                var sum=0;\r\n                for(var item of this.cartlist){\r\n                    sum=sum+item.price*item.count;\r\n                }\r\n                return sum;\r\n            }\r\n        },\r\n        components:{\r\n            \"twolist-box\":twolist\r\n        }\r\n    }\r\n</script>\r\n\r\n\r\n<style>\r\n    .list-container .mui-numbox .mui-input-numbox{\r\n        border:0 !important;\r\n        width: 30px !important;\r\n        height: 20px !important;\r\n    }\r\n    .list-container img{\r\n        width:20%;\r\n    }\r\n    .list-container .mui-numbox{\r\n        border: 0;\r\n        background: #fff;\r\n        float: right;\r\n    }\r\n    .list-container .mui-numbox .right{\r\n        background:lightblue;\r\n        border-radius:50%;\r\n        width: 21% !important;\r\n        height: 60%;\r\n        line-height: 23px;\r\n    }\r\n    .list-container .mui-numbox .left{\r\n        background:lightblue;\r\n        border-radius:50%;\r\n        width: 21% !important;\r\n        height: 60%;\r\n        line-height: 18px;\r\n    }\r\n    .list-container .sumscore{\r\n        font-size:20px;\r\n        color:purple;\r\n        font-weight:bold;\r\n    }\r\n</style>"]}]}