\n                </router-link>\r\n                <div class=\"info\">\r\n                    <h3 class=\"title\">{{item.title}}</h3>\r\n                    <h3 class=\"text\">{{item.text}}</h3>\r\n                    <p class=\"price\">\r\n                        <span>￥{{item.price}}</span>\r\n                        <span class=\"size\">/{{item.size}}</span>\r\n                    </p>\r\n                </div> \r\n            </li>\r\n        </ul>\r\n      </mt-tab-container-item>\r\n    </mt-tab-container>\r\n  </div>\r\n</template>\r\n \r\n<script>\r\n\r\nexport default {\r\n  name: 'page-navbar',\r\n \r\n  data() {\r\n    return {\r\n      selected: '1',\r\n      list1:[],\r\n      list2:[],\r\n      list3:[],\r\n    };\r\n  },\r\n     methods:{\r\n            getListCake1(){\r\n                this.$http.get(\"cakelists3?pid=1\").then(result=>{\r\n                this.list1=result.body\r\n                //console.log(this.list)\r\n            })\r\n           },\r\n           getListCake2(){\r\n                this.$http.get(\"cakelists3?pid=2\").then(result=>{\r\n                this.list2=result.body\r\n               // console.log(this.list)\r\n            })\r\n           },\r\n           getListCake3(){\r\n                this.$http.get(\"cakelists3?pid=3\").then(result=>{\r\n                this.list3=result.body\r\n               // console.log(this.list)\r\n            })\r\n           },   \r\n        },\r\n        created(){\r\n            this.getListCake1();\r\n            this.getListCake2();\r\n            this.getListCake3();\r\n        },\r\n  components:{\r\n\r\n        }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n    .navbar2 .lis img{\r\n        width: 145px;\r\n        margin-left: -225px;\r\n    }\r\n    .navbar2 .info{\r\n        width: 36%;\r\n        margin-left: 16px;\r\n    }\r\n    .navbar2 .info h3{\r\n        font-size: 12px !important;\r\n      }\r\n</style>"]}]}